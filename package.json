{
  "name": "javascript-development-environment",
  "version": "0.0.1",
  "eslintConfig": {},
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src security-check test:watch lint:watch",
    "open:src": "nodemon buildScripts/srcServer.js",
    "security-check": "nsp check",
    "lint": "esw webpack.config.* public routes utils buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress \"routes/**/*.test.js\" \"public/**/*.test.js\"  \"utils/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Ã‰tienne Rouge",
  "license": "Copyright",
  "dependencies": {},
  "devDependencies": {
    "watchify": "3.9.0",
    "browserify": "14.5.0",
    "google-auth-library": "0.12.0",
    "browserify-middleware": "8.0.0",
    "googleapis": "23.0.0",
    "chai": "4.1.2",
    "chalk": "2.3.0",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.1",
    "eslint": "4.14.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-watch": "3.1.3",
    "express": "4.16.2",
    "jsdom": "11.5.1",
    "json-schema-faker": "0.4.6",
    "json-server": "0.12.1",
    "mocha": "4.1.0",
    "nock": "9.1.5",
    "npm-run-all": "4.1.2",
    "nsp": "3.1.0",
    "numeral": "2.0.6",
    "open": "0.0.5",
    "rimraf": "2.6.2",
    "surge": "0.19.0",
    "pug": "2.0.0-rc.4",
    "browserify-fs": "1.0.0",
    "lowdb": "1.0.0",
    "body-parser": "1.18.2",
    "shortid": "2.2.8",
    "supertest": "3.0.0",
    "jquery": "3.3.1"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "data/*",
      "public/*",
      "views/*",
      "*.test.js"
    ]
  }
}
